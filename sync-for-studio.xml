"use client";

import { useAuth } from "@/context/AuthContext";
import { useRouter } from "next/navigation";
import { useEffect } from "react";

// This component uses inline styles and pure CSS animations
// to prevent any flash of unstyled content (FOUC).
const SplashScreen = () => (
  <>
    <style jsx>{`
      .splash-container {
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        min-height: 100vh;
        background-color: hsl(var(--background));
        padding: 1rem;
        box-sizing: border-box;
      }
      .z-path {
        stroke-dasharray: 300;
        stroke-dashoffset: 300;
        animation: drawZ 2.5s ease-out forwards;
        filter: drop-shadow(0 0 8px hsl(var(--primary) / 0.5));
      }
      .z-dot {
        opacity: 0;
        transform: scale(0);
        transform-origin: center;
        animation: popIn 0.5s ease-out 2s forwards;
      }
      @keyframes drawZ {
        to {
          stroke-dashoffset: 0;
        }
      }
      @keyframes popIn {
        0% {
          opacity: 0;
          transform: scale(0);
        }
        100% {
          opacity: 1;
          transform: scale(1);
        }
      }
    `}</style>
    <div className="splash-container">
       <svg viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg" width="150" height="150">
        <defs>
          <linearGradient id="zenitBlueGradient" x1="0%" y1="0%" x2="100%" y2="100%">
            <stop offset="0%" stopColor="hsl(var(--primary))" />
            <stop offset="100%" stopColor="hsl(var(--accent))" />
          </linearGradient>
        </defs>
        <path d="M 15 25 H 80 L 30 80 H 65"
              className="z-path"
              stroke="url(#zenitBlueGradient)"
              strokeWidth="8"
              fill="none"
              strokeLinecap="round"
              strokeLinejoin="round"/>
        <circle cx="75" cy="28" r="5" fill="hsl(var(--accent))" className="z-dot" />
      </svg>
      <h1 style={{
        fontSize: '2.25rem',
        lineHeight: '2.5rem',
        fontFamily: 'var(--font-headline)',
        fontWeight: 'bold',
        color: 'hsl(var(--foreground))',
        marginTop: '1rem'
      }}>
        Zenit Tracker
      </h1>
      <p style={{
        color: 'hsl(var(--muted-foreground))',
        marginTop: '0.5rem'
      }}>
        Precision in Every Test.
      </p>
    </div>
  </>
);

export default function RootPage() {
  const { user, loading } = useAuth();
  const router = useRouter();

  useEffect(() => {
    if (!loading) {
      const timer = setTimeout(() => {
        if (user) {
          router.replace('/dashboard');
        } else {
          router.replace('/login');
        }
      }, 3000); // 3-second delay to ensure animation completes

      return () => clearTimeout(timer); // Cleanup timer on unmount
    }
  }, [user, loading, router]);

  return <SplashScreen />;
}
